{
  "name": "atomic10-components",
  "description": "Atomic10 is a React component library with clean, contemporary, and customizable components.",
  "version": "1.2.2",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jameson13B/atomic10-components"
  },
  "dependencies": {
    "react-toggle": "^4.0.2",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
